{
  "name": "daedalus-data",
  "type": "module",
  "version": "1.0.0-dev.5",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexv710/daedalusData.git"
  },
  "scripts": {
    "build": "nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "lint:ci": "eslint --ignore-pattern '.nuxt/**' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.spec.ts' .",
    "lint:fix": "eslint --ignore-pattern '.nuxt/**' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.spec.ts' --fix .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "commit": "cz",
    "prepare": "cd .. && husky install frontend/.husky",
    "postinstall": "pinst --enable",
    "prepack": "pinst --disable"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "sharp": "^0.33.5",
    "three": "^0.173.0",
    "uuid": "^11.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.21.0",
    "@iconify-json/carbon": "^1.2.6",
    "@iconify-json/twemoji": "^1.2.2",
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/eslint": "^1.0.1",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.9.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@unocss/eslint-config": "^65.4.3",
    "@unocss/nuxt": "^65.4.3",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^12.5.0",
    "commitizen": "^4.3.1",
    "consola": "^3.4.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^1.0.1",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.1",
    "husky": "^8.0.0",
    "jsdom": "^26.0.0",
    "nuxt": "3.15.4",
    "pinia": "^2.3.1",
    "pinst": "^3.0.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.0",
    "vuetify-nuxt-module": "^0.18.3"
  },
  "resolutions": {
    "@nuxt/devtools": "^2.0.0",
    "unplugin": "^2.1.2",
    "vite": "^6.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
